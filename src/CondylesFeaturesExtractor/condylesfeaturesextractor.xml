<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Shape Analysis</category>
  <title>Condyles Features Creator</title>
  <description> Specific features are computed at each point of the 3D Model </description>
  <version>1.0</version>
  <contributor>Priscille de Dumast (University of Michigan)</contributor>
  <acknowledgements>  </acknowledgements>
    <parameters>
      <label>I/O</label>

      <geometry fileExtensions=".vtk" multiple="false">
        <name>inputSurface</name>
        <label>Input surface</label>
        <longflag>input</longflag>
        <description>Input surface</description>
      </geometry>

      <geometry fileExtensions=".vtk" multiple="false">
        <name>outputSurface</name>
        <label>Output surface</label>
        <longflag>output</longflag>
        <description>Output surface</description>
      </geometry>

      <geometry fileExtensions=".vtk" multiple="true">
        <name>meanGroupDir</name>
        <label>Mean group directory</label>
        <longflag>meanGroup</longflag>
        <description>Mean group directory</description>
      </geometry>
    
    </parameters>
</executable>